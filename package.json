{
  "name": "payszn-sdk",
  "version": "0.2.11",
  "private": false,
  "main": "dist/sdk/index.js",
  "types": "types/sdk/index.d.ts",
  "files": [
    "dist/**/*",
    "types/**/*",
    "styles.css"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:sdk": "tsc --skipLibCheck && babel dist --out-dir dist --extensions \".js,.jsx\" --copy-files && npm run copy-css && npm run copy-assets",
    "copy-css": "cpx \"src/**/*.css\" dist",
    "copy-assets": "cpx \"src/sdk/assets/**/*\" dist/sdk/assets",
    "version:patch": "npm run build:sdk && npm version patch && npm publish",
    "version:minor": "npm run build:sdk && npm version minor && npm publish",
    "version:major": "npm run build:sdk && npm version major && npm publish"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.0.0",
    "@metaplex-foundation/umi-bundle-defaults": "^1.0.0",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@solana/spl-token": "^0.4.12",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.98.0",
    "axios": "^1.7.9",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-fetch": "^4.1.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "browserify-zlib": "^0.2.0",
    "cpx": "^1.5.0",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pino-pretty": "^13.0.0",
    "postcss": "^8",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
